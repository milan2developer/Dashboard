{
    "name": "dashboard",
    "author": "Kalantak Technology",
    "description": "This Application is for Analysis Purpose",
    "version": "0.0.1",
    "license": "COMMERCIAL",
    "main": "main.js",
    "angular-cli": {},
    "scripts": {
        "ng": "ng",
        "start": "node server.js",
        "build": "ng build --prod --base-href && npm run copy",
        "copy": "copyfiles \"README.md\" \"dist/dashboard/\" && copyfiles \"main.js\" \"dist/dashboard/\"",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron": "concurrently \"npm run server\" \"ng build && electron .\"",
        "dist": "electron-builder",
        "heroku-postbuild": "ng build --prod --base-href"
    },
    "private": true,
    "engines": {
        "node": "12.20.0",
        "npm": "6.14.6"
    },
    "dependencies": {
        "@angular/animations": "~13.0.1",
        "@angular/cdk": "~13.0.1",
        "@angular/common": "~13.0.1",
        "@angular/compiler": "~13.0.1",
        "@angular/core": "~13.0.1",
        "@angular/forms": "~13.0.1",
        "@angular/platform-browser": "~13.0.1",
        "@angular/platform-browser-dynamic": "~13.0.1",
        "@angular/router": "~13.0.1",
        "@ngrx/effects": "^13.0.0",
        "@ngrx/entity": "^13.0.2",
        "@ngrx/router-store": "^13.0.2",
        "@ngrx/store": "^13.0.2",
        "@ngrx/store-devtools": "^13.0.0",
        "@types/d3-collection": "^1.0.10",
        "@types/d3-sankey": "^0.11.2",
        "body-parser": "^1.20.0",
        "cors": "^2.8.5",
        "d3": "^7.6.1",
        "d3-cloud": "^1.2.5",
        "d3-collection": "^1.0.7",
        "d3-sankey": "^0.12.3",
        "d3-tile": "^1.0.0",
        "express": "^4.18.1",
        "highcharts": "^10.1.0",
        "highcharts-angular": "^3.0.0",
        "ngx-electron": "^2.2.0",
        "path": "^0.12.7",
        "primeflex": "3.1.0",
        "primeicons": "5.0.0",
        "primeng": "13.0.2",
        "prismjs": "^1.28.0",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.3.8",
        "@angular/cli": "~13.0.1",
        "@angular/compiler-cli": "~13.0.1",
        "@types/d3": "^7.4.0",
        "@types/d3-cloud": "^1.2.5",
        "@types/jasmine": "~3.6.0",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "^12.11.1",
        "codelyzer": "^0.0.28",
        "copyfiles": "^2.4.1",
        "electron": "^20.0.3",
        "electron-builder": "20.44.1",
        "electron-installer-windows": "^3.0.0",
        "electron-packager": "^15.5.2",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "^6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.3",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "nodemon": "^2.0.19",
        "protractor": "~7.0.0",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.4.3"
    },
    "build": {
        "appId": "Info@Kalantak.com",
        "productName": "dashboard",
        "win": {
            "icon": "assets/images/K1.jpg",
            "publisherName": "Kalantak Technology",
            "target": [
                "nsis"
            ]
        },
        "linux": {
            "icon": "assets/images/K1.jpg",
            "target": [
                "AppImage",
                "tar.gz"
            ]
        },
        "mac": {
            "icon": "assets/images/K1.jpg",
            "target": [
                "zip",
                "dmg"
            ]
        },
        "directories": {
            "output": "build"
        },
        "extraResources": [
            {
                "from": "../assets",
                "to": "assets",
                "filter": []
            }
        ],
        "asar": false,
        "nsis": {
            "oneClick": false,
            "perMachine": false,
            "allowToChangeInstallationDirectory": true,
            "createDesktopShortcut": true,
            "createStartMenuShortcut": true,
            "allowElevation": false
        }
    }
}
